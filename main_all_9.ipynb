    "import re\n",
    "\n",
    "def extract_categories(filename):\n",
    "    # 使用正则表达式匹配文件名格式\n",
    "    match = re.match(r'(\\d+-.+?) - (.+)', filename)\n",
    "    if match:\n",
    "        main_category = match.group(1).strip()\n",
    "        sub_category = match.group(2).strip()\n",
    "        return main_category, sub_category\n",
    "    else:\n",
    "        return None, None"
   ]
